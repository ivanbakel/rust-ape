// run-pass
#![feature(use_extern_macro)]
#![feature(proc_macro_hygiene)]
extern crate ape;
extern crate ape_runtime;

use ape::arb_peg;

fn main() {
  let pattern = arb_peg!{true};

  let sequence = vec![true, false, true].into_iter();

  assert!(pattern.pmatch(sequence).is_ok());
}
